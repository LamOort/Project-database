CREATE TABLE Person (id int(11) NOT NULL AUTO_INCREMENT, Code varchar(7) NOT NULL UNIQUE, Name varchar(50) NOT NULL UNIQUE, PRIMARY KEY (id)) CHARACTER SET UTF8;
CREATE TABLE Course (id int(11) NOT NULL AUTO_INCREMENT, Code varchar(50), Name varchar(50), Changes varchar(50), Languages varchar(50), Credit int(11), `Size` int(11), P1 int(11), P2 int(11), P3 int(11), P4 int(11), P5 int(11), Total int(11), Curriculumid int(11) NOT NULL, PRIMARY KEY (id)) CHARACTER SET UTF8;
