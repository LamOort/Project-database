CREATE TABLE SubGroup (id int(11) NOT NULL AUTO_INCREMENT, Code varchar(20), Department varchar(30), Groupid int(11) NOT NULL, PRIMARY KEY (id)) CHARACTER SET UTF8;
CREATE TABLE Implementation (id int(11) NOT NULL AUTO_INCREMENT, Courseid int(11) NOT NULL, Personid int(11) NOT NULL, Roomid int(11) NOT NULL, SubGroupid int(11) NOT NULL, PRIMARY KEY (id)) CHARACTER SET UTF8;
CREATE TABLE Person_Implementation (Personid int(11) NOT NULL, Implementationid int(11) NOT NULL, PRIMARY KEY (Personid, Implementationid)) CHARACTER SET UTF8;
